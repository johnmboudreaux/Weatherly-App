{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-multiple-empty-lines": 0,
    "max-len": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "arrow-body-style": 0,
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0]
  }
}
